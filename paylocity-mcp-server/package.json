{
  "name": "paylocity-nextgen-api",
  "version": "v1",
  "description": "MCP Server generated from OpenAPI spec for Paylocity NextGen Apis",
  "private": true,
  "type": "module",
  "main": "build/index.js",
  "files": [
    "build",
    "src"
  ],
  "scripts": {
    "start": "node build/index.js",
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "prestart": "npm run build",
    "start:web": "node build/index.js --transport=web"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.9.0",
    "axios": "^1.8.4",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "json-schema-to-zod": "^2.6.1",
    "uuid": "^11.1.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^18.19.0",
    "@types/uuid": "^10.0.0",
    "typescript": "^5.8.3"
  }
}
